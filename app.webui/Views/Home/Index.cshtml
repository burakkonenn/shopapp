@model ProductModel
@await Html.PartialAsync("_header")
@await Html.PartialAsync("_navbar")
@await Html.PartialAsync("_headerTwo")
<div class="row">
    <div class="col-md-12">
       <div class="row">
           @foreach (var item in Model.Products)
           {
               <div class="col-md-3">
                   @await Html.PartialAsync("_products", item)
               </div>
           }
       </div>
    </div>
</div>
